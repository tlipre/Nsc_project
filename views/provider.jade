ghtml
  head
    script(src="https://cdn.socket.io/socket.io-1.3.3.js")
  body
    textarea(id="editor" cols="100" rows="20")
    script(src="https://code.jquery.com/jquery-2.1.3.min.js")
    script.
      socket = io();
      base = new Date().getTime()
      $(document).on("keyup", "#editor", function(){
        newBase = new Date().getTime()
        socket.emit('editor', {'word':$("#editor").val(),'delay':newBase-base})
        base = newBase
      })
